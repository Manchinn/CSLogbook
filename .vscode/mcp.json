{
    "servers": {
        "sequential-thinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "mcp_server_mysql": {
            "command": "npx",
            "args": [
                "-y",
                "@benborla29/mcp-server-mysql"
            ],
            "env": {
                "MYSQL_HOST": "127.0.0.1",
                "MYSQL_PORT": "3306",
                "MYSQL_USER": "root",
                "MYSQL_PASS": "root",
                "MYSQL_DB": "cslogbook_local_dev",
                
            }
        },
    },
    "inputs": [
        {
            "id": "mysql_hostname",
            "type": "promptString",
            "description": "Enter the MySQL Hostname"
        },
        {
            "id": "mysql_port",
            "type": "promptString",
            "description": "Enter the MySQL Port Number"
        },
        {
            "id": "mysql_username",
            "type": "promptString",
            "description": "Enter the MySQL Username"
        },
        {
            "id": "mysql_username",
            "type": "promptString",
            "description": "Enter the MySQL Username"
        },
    ]
}